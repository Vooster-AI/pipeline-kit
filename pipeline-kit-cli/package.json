{
  "name": "pipeline-kit",
  "version": "0.1.8",
  "description": "AI agent pipeline orchestration CLI",
  "license": "MIT",
  "type": "module",
  "bin": {
    "pipeline-kit": "bin/pipeline-kit.js"
  },
  "scripts": {
    "postinstall": "node scripts/install.js",
    "prepack": "node scripts/clean_vendor.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "publish:public": "npm publish --access public"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "bin",
    "lib",
    "scripts",
    "vendor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vooster-AI/pipeline-kit.git",
    "directory": "pipeline-kit-cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ai",
    "agent",
    "pipeline",
    "orchestration",
    "cli"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "tar": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^3.2.4",
    "mock-fs": "^5.2.0",
    "nock": "^13.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
